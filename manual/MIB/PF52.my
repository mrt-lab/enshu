        backUp-Lock(5)
    }
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
        "The state of GSRP group.."
    ::= { pf5000GsrpVlanGroupEntry 3 }

pf5000GsrpPriority OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
        "Configured priority."
    ::= { pf5000GsrpVlanGroupEntry 4 }

pf5000GsrpActivePorts OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
