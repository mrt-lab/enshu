PF5000-PF5200-TRAP DEFINITIONS ::= BEGIN
IMPORTS
enterprises
    FROM RFC1155-SMI
TRAP-TYPE
    FROM RFC-1215
ifIndex
    FROM RFC1213-MIB
pf5200, pf5200TrapId,
pf5000AxrpGroupRingId, pf5000AxrpGroupMode, pf5000AxrpGroupRingAttribute,
pf5000AxrpGroupMonitoringState, pf5000AxrpGroupRingport1, pf5000AxrpGroupRingport2,
pf5200SystemMsgType, pf5200SystemMsgTimeStamp,pf5200SystemMsgLevel,
pf5200SystemMsgEventPoint, pf5200SystemMsgEventInterfaceID,
pf5200SystemMsgEventCode, pf5200SystemMsgAdditionalCode, pf5200SystemMsgText,
pf5200ChassisIndex, pf5200TemperatureStatusIndex, pf5200TemperatureStatusDescr,
pf5200TemperatureStatusValue, pf5200TemperatureState,
pf5000GsrpGroupId, pf5000GsrpVlanGroupId, pf5000GsrpState,
pf5000LoginName, pf5000LoginTime, pf5000LoginLocation, pf5000LoginLine,
pf5000LoginFailureTime, pf5000LogoutTime, pf5000LogoutStatus,
pf5000L2ldPortIndex, pf5000L2ldPortIfIndex, pf5000L2ldPortSourcePortIfindex,
pf5000L2ldPortDestinationPortIfindex, pf5000L2ldPortSourceVlan
    FROM PF5200
;

