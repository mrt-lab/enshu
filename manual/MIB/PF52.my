pf5000GsrpSelectionPattern OBJECT-TYPE
    SYNTAX  INTEGER {
        ports-Priority-MAC(1),
        priority-Ports-MAC(2)
    }
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
        "Master/Backup selection pattern. Ports-Priority-MAC(1)  Priority-Ports-MAC(2)"
