    ::= { pf5200FanEntry 3 }

pf5200PhysLineTable OBJECT-TYPE
    SYNTAX  SEQUENCE OF Pf5200PhysLineTableEntry
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
        "A table containing physical port information."
    ::= { pf5200PhysLine 1 }

pf5200PhysLineEntry OBJECT-TYPE
    SYNTAX  Pf5200PhysLineTableEntry
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
        "An entry containing physical port."
    INDEX { pf5200ChassisIndex, pf5200PhysLineIndex }
    ::= { pf5200PhysLineTable 1 }

    Pf5200PhysLineTableEntry ::=
        SEQUENCE {
            pf5200PhysLineIndex
                INTEGER,
            pf5200PhysLineConnectorType
                INTEGER,
            pf5200PhysLineOperStatus
                INTEGER,
            pf5200PhysLineIfIndexNumber
                INTEGER,
            pf5200PhysLineTransceiverStatus
                INTEGER
        }

pf5200PhysLineIndex OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
        "Physical port number. The value is from 1 to pf5200NifLineNumber."
    ::= { pf5200PhysLineEntry 1 }

pf5200PhysLineConnectorType OBJECT-TYPE
    SYNTAX  INTEGER {
        other(1),
        type1000BASE-LX(301),
        type1000BASE-SX(302),
        type1000BASE-LH(303),
        type10GBASE-SR(401),
        type10GBASE-LR(402),
        type10GBASE-ER(403),
        type10GBASE-CU1M(405),
        type10GBASE-CU3M(406),
        type10GBASE-CU5M(407)
    }
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
        "Connector type is connected physical port. The value is 1(other)
        when connector is disconnected or connector type is unknown."
    ::= { pf5200PhysLineEntry 2 }

pf5200PhysLineOperStatus OBJECT-TYPE
    SYNTAX  INTEGER {
        other(1),
        active(2),
        initialization(3),
        down(4),
        lock(6),
        close(7),
        line-fault(8),
        test(9),
        nothing-configuration(10)
    }
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
        "Status of physical port. The value is -1 when PSU or NIF board is
        disconnected."
    ::= { pf5200PhysLineEntry 3 }

pf5200PhysLineIfIndexNumber OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
        "Number of ifIndex which is included in physical port.The value is -1
        when PSU board or NIF board is disconnected."
    ::= { pf5200PhysLineEntry 4 }

pf5200PhysLineTransceiverStatus OBJECT-TYPE
    SYNTAX  INTEGER {
        unchangeable-transceiver(1),
        sfp-mounted(20),
        sfp-unmounted(21),
        unsupported-sfp-mounted(22),
        sfp-status-unknown(23)
    }
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
        "Type and status of replaceable transceiver."
    ::= { pf5200PhysLineEntry 5 }
--
-- pf5200FdbClearMIB      OBJECT IDENTIFIER ::= { pf5200OperationCommand 1 }
--

pf5200FdbClearSet OBJECT-TYPE
    SYNTAX  INTEGER {
        initialValue(0),
        processing(1),
        failure(2),
        success(3)
    }
    ACCESS  read-write
    STATUS  mandatory
    DESCRIPTION
        "Fdb clear information. When want to clear fdb, 
        then set value of this MIB as 1."
    ::= { pf5200FdbClearMIB 1 }

pf5200FdbClearReqTime OBJECT-TYPE
    SYNTAX  TimeTicks
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
        "This object indicates the most recent time that
        received the demand to clear Fdb information.(sysUpTime)"
    ::= { pf5200FdbClearMIB 2 }

pf5200FdbClearSuccessTime OBJECT-TYPE
    SYNTAX  TimeTicks
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
        "This object indicates the time of the most recent
         Fdb clearing success.(sysUpTime)"
    ::= { pf5200FdbClearMIB 3 }

pf5200SystemManageReload OBJECT-TYPE
    SYNTAX  INTEGER {
        initialValue(0),
        restart(1),
        standby(2)
    }
    ACCESS  read-write
    STATUS  mandatory
    DESCRIPTION
        "System reload information. When want to restart or standby, 
        then set value of this MIB as 1 or 2."
    ::= { pf5200SystemManagementMIB 1 }

pf5000OspfVirtIfStateChange TRAP-TYPE
    ENTERPRISE  pf5000OspfTraps
    VARIABLES {
        pf5000OspfVirtIfDomainNumber,
        pf5000OspfRouterId,
        pf5000OspfVirtIfAreaId,
        pf5000OspfVirtIfNeighbor,
        pf5000OspfVirtIfState
    }
    DESCRIPTION
        "An pf5000OspfIfStateChange trap signifies that there has  been a change 
        in the state of an OSPF virtual interface."
    ::= 1

pf5000OspfNbrStateChange TRAP-TYPE
    ENTERPRISE  pf5000OspfTraps
    VARIABLES {
        pf5000OspfNbrDomainNumber,
        pf5000OspfRouterId,
        pf5000OspfNbrIpAddr,
        pf5000OspfNbrAddressLessIndex,
        pf5000OspfNbrRtrId,
        pf5000OspfNbrState
    }
    DESCRIPTION
        "An pf5000OspfNbrStateChange trap signifies that
        there  has been a change in the state of a non-
        virtual OSPF neighbor.   This  trap  should  be
        generated  when  the  neighbor  state regresses
        (e.g., goes from Attempt or Full  to  1-Way  or
        Down)  or progresses to a terminal state (e.g.,
        2-Way or Full).  When an  neighbor  transitions
        from  or  to Full on non-broadcast multi-access
        and broadcast networks, the trap should be gen-
        erated  by the designated router.  A designated
        router transitioning to Down will be  noted  by
        pf5000OspfIfStateChange."
    ::= 2

pf5000OspfVirtNbrStateChange TRAP-TYPE
    ENTERPRISE  pf5000OspfTraps
    VARIABLES {
        pf5000OspfVirtNbrDomainNumber,
        pf5000OspfRouterId,
        pf5000OspfVirtNbrArea,
        pf5000OspfVirtNbrRtrId,
        pf5000OspfVirtNbrState
    }
    DESCRIPTION
        "An pf5000OspfIfStateChange trap signifies that there
        has  been a change in the state of an OSPF vir-
        tual neighbor.  This trap should  be  generated
        when  the  neighbor state regresses (e.g., goes
        from Attempt or  Full  to  1-Way  or  Down)  or
        progresses to a terminal state (e.g., Full)."
    ::= 3

pf5000OspfIfConfigError TRAP-TYPE
    ENTERPRISE  pf5000OspfTraps
    VARIABLES {
        pf5000OspfIfDomainNumber,
        pf5000OspfRouterId,
        pf5000OspfIfIpAddress,
        pf5000OspfAddressLessIf,
        pf5000OspfPacketSrc,
        pf5000OspfConfigErrorType,
        pf5000OspfPacketType
    }
    DESCRIPTION
        "An pf5000OspfIfConfigError  trap  signifies  that  a
        packet  has  been received on a non-virtual in-
        terface  from  a  router  whose   configuration
        parameters  conflict  with this router's confi-
        guration parameters.  Note that the  event  op-
        tionMismatch  should  cause  a  trap only if it
        prevents an adjacency from forming."
    ::= 4

pf5000OspfVirtIfConfigError TRAP-TYPE
    ENTERPRISE  pf5000OspfTraps
    VARIABLES {
        pf5000OspfVirtIfDomainNumber,
        pf5000OspfRouterId,
        pf5000OspfVirtIfAreaId,
        pf5000OspfVirtIfNeighbor,
        pf5000OspfConfigErrorType,
        pf5000OspfPacketType
    }
    DESCRIPTION
        "An pf5000OspfConfigError trap signifies that a pack-
        et  has  been  received  on a virtual interface
        from a router  whose  configuration  parameters
        conflict   with   this  router's  configuration
        parameters.  Note that the event optionMismatch
        should  cause a trap only if it prevents an ad-
        jacency from forming."
    ::= 5

pf5000OspfIfAuthFailure TRAP-TYPE
    ENTERPRISE  pf5000OspfTraps
    VARIABLES {
        pf5000OspfIfDomainNumber,
        pf5000OspfRouterId,
        pf5000OspfIfIpAddress,
        pf5000OspfAddressLessIf,
        pf5000OspfPacketSrc,
        pf5000OspfConfigErrorType,
        pf5000OspfPacketType
    }
    DESCRIPTION
        "An pf5000OspfIfAuthFailure  trap  signifies  that  a
        packet  has  been received on a non-virtual in-
        terface from a router whose authentication  key
        or  authentication  type  conflicts  with  this
        router's authentication key  or  authentication
        type."
    ::= 6

pf5000OspfVirtIfAuthFailure TRAP-TYPE
    ENTERPRISE  pf5000OspfTraps
    VARIABLES {
        pf5000OspfVirtIfDomainNumber,
        pf5000OspfRouterId,
        pf5000OspfVirtIfAreaId,
        pf5000OspfVirtIfNeighbor,
        pf5000OspfConfigErrorType,
        pf5000OspfPacketType
    }
    DESCRIPTION
        "An pf5000OspfVirtIfAuthFailure trap signifies that a
        packet has been received on a virtual interface
        from a router whose authentication key  or  au-
        thentication  type conflicts with this router's
        authentication key or authentication type."
    ::= 7

pf5000OspfIfStateChange TRAP-TYPE
    ENTERPRISE  pf5000OspfTraps
    VARIABLES {
        pf5000OspfIfDomainNumber,
        pf5000OspfRouterId,
        pf5000OspfIfIpAddress,
        pf5000OspfAddressLessIf,
        pf5000OspfIfState
    }
    DESCRIPTION
        "An pf5000OspfIfStateChange trap signifies that there
        has been a change in the state of a non-virtual
        OSPF interface. This trap should  be  generated
        when  the interface state regresses (e.g., goes
        from Dr to Down) or progresses  to  a  terminal
        state  (i.e.,  Point-to-Point, DR Other, Dr, or
        Backup)."
    ::= 16 

pf5000OadpNeighborCachelastChangeTrap TRAP-TYPE
    ENTERPRISE  pf5000OadpMIBNotifications
    VARIABLES   { pf5000OadpNeighborCacheLastChange }
    DESCRIPTION
        "This trap indicates that information of OADP neighbor nodes is updated."
    ::= 1 

