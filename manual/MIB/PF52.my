    SYNTAX  INTEGER {
        down (1),
        attempt (2),
        init (3),
        twoWay (4),
        exchangeStart (5),
        exchange (6),
        loading (7),
        full (8)
    }
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
        "The State of the relationship with this Neighbor."
    ::= { pf5000OspfNbrEntry 7 }

pf5000OspfNbrEvents OBJECT-TYPE
    SYNTAX  Counter
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
        "The number of times this neighbor relationship
        has changed state, or an error has occurred."
