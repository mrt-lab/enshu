pf5000IfStatsTable OBJECT-TYPE
    SYNTAX  SEQUENCE OF Pf5000IfStatsTableEntry
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
        "A table containing extension statistics for more than 100,000,000
        bps interface."
    ::= { pf5000IfStats 1 }

pf5000IfStatsEntry OBJECT-TYPE
    SYNTAX  Pf5000IfStatsTableEntry
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
        "An entry containing extension statistics for interface."
    INDEX   { pf5000IfStatsIndex }
    ::= { pf5000IfStatsTable 1 }

    Pf5000IfStatsTableEntry ::=
        SEQUENCE {
            pf5000IfStatsIndex
                INTEGER,
            pf5000IfStatsName
                DisplayString,
            pf5000IfStatsInMegaOctets
                Counter,
            pf5000IfStatsInUcastMegaPkts
                Counter,
            pf5000IfStatsInMulticastMegaPkts
                Counter,
            pf5000IfStatsInBroadcastMegaPkts
                Counter,
            pf5000IfStatsOutMegaOctets
                Counter,
            pf5000IfStatsOutUcastMegaPkts
                Counter,
            pf5000IfStatsOutMulticastMegaPkts
                Counter,
            pf5000IfStatsOutBroadcastMegaPkts
                Counter,
            pf5000IfStatsHighSpeed
                Counter
        }

pf5000IfStatsIndex OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
        "This object value is same as Ifindex which interface index
        of a system."
    ::= { pf5000IfStatsEntry 1 }

pf5000IfStatsName OBJECT-TYPE
    SYNTAX  DisplayString
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
        "Interfaces name. Same as ifDescr."
    ::= { pf5000IfStatsEntry 2 }

pf5000IfStatsInMegaOctets OBJECT-TYPE
    SYNTAX  Counter
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
        "Number of octets that received on the interface. This value is
        displayed in mega-octets(M). Value of 0 through 999,999 are rounded
        down."
    ::= { pf5000IfStatsEntry 3 }

pf5000IfStatsInUcastMegaPkts OBJECT-TYPE
    SYNTAX  Counter
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
        "Number of subnetwork-unicast packets that received on the interface.
        This value is displayed in mega-packets(M).Value of 0 through 999,999
        are rounded down."
    ::= { pf5000IfStatsEntry 4 }

pf5000IfStatsInMulticastMegaPkts OBJECT-TYPE
    SYNTAX  Counter
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
        "Number of multicast mega-packets that received on the interface.
        this value is displayed in mega-packets(M).Value of 0 through 999,999
        are rounded down."
    ::= { pf5000IfStatsEntry 5 }


pf5000IfStatsInBroadcastMegaPkts OBJECT-TYPE
    SYNTAX  Counter
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
        "The number of non-unicast packets delivered to a higher-layer
        protocol. The value is displayed in mega-packets (M). Value of 0
        through 999,999 are rounded down. "
    ::= { pf5000IfStatsEntry 6 }

pf5000IfStatsOutMegaOctets OBJECT-TYPE
    SYNTAX  Counter
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
        "The total number of octets transmitted out of the interface.
        The value is displayed in mega-octets(M). Value of 0 through 999,999
        are rounded down."
    ::= { pf5000IfStatsEntry 7 }

pf5000IfStatsOutUcastMegaPkts OBJECT-TYPE
    SYNTAX  Counter
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
        "The total number of packets that higher-level protocols requested
        be transmitted to a subnetwork-unicast address.The value is displayed
        in mega packets(M). Value of 0 through 999,999 are rounded down."
    ::= { pf5000IfStatsEntry 8 }

pf5000IfStatsOutMulticastMegaPkts OBJECT-TYPE
    SYNTAX  Counter
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
        "Number of multicast packets that transmitted on the interface.
        This value is displayed in mega-packets(M). Value of 0 through
        999,999 are rounded down."
    ::= { pf5000IfStatsEntry 9 }

pf5000IfStatsOutBroadcastMegaPkts OBJECT-TYPE
    SYNTAX  Counter
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
        "Number of broadcast packets that transmitted on the interface.
        This value is displayed in mega-packets(M). Value of 0 through
        999,999 are rounded down. "
    ::= { pf5000IfStatsEntry 10 }

pf5000IfStatsHighSpeed OBJECT-TYPE
    SYNTAX  Counter
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
        "A circuit speed.This value is displayed in mega-bps[Mbps].
        Round off the 1,000,000bps. "
    ::= { pf5000IfStatsEntry 11 }

--  QoS interface statistics Variables

pf5000EtherTxQoSStatsTable OBJECT-TYPE
    SYNTAX  SEQUENCE OF Pf5000EtherTxQoSStatsEntry
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
        "A collection of QoS statistics kept for a particular
        Ethernet interface."
    ::= { pf5000EtherTxQoS 1 }

pf5000EtherTxQoSStatsEntry OBJECT-TYPE
    SYNTAX  Pf5000EtherTxQoSStatsEntry
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
        "An entry containing QoS statistics for a particular 
        Ethernet interface."
    INDEX { pf5000EtherTxQoSStatsIndex }
    ::= { pf5000EtherTxQoSStatsTable 1 }

    Pf5000EtherTxQoSStatsEntry ::=
        SEQUENCE {
            pf5000EtherTxQoSStatsIndex
                INTEGER,
            pf5000EtherTxQoSStatsMaxQnum
                INTEGER,
            pf5000EtherTxQoSStatsLimitQlen
                INTEGER,
            pf5000EtherTxQoSStatsTotalOutFrames
                Counter,
            pf5000EtherTxQoSStatsTotalOutBytesHigh
                Counter,
            pf5000EtherTxQoSStatsTotalOutBytesLow
                Counter,
            pf5000EtherTxQoSStatsTotalDiscardFrames
                Counter
        }

pf5000EtherTxQoSStatsIndex OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
        "Identified by a particular value of this
        index is same value of ifIndex.Its value ranges
        between 1 and the value of ifNumber."
    ::= { pf5000EtherTxQoSStatsEntry 1 }

pf5000EtherTxQoSStatsMaxQnum OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
        "Max length of queue for an ethernet interface."
    ::= { pf5000EtherTxQoSStatsEntry 2 }

pf5000EtherTxQoSStatsLimitQlen OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
        "The limit length of outband queue for an ethernet interface."
    ::= { pf5000EtherTxQoSStatsEntry 3 }

pf5000EtherTxQoSStatsTotalOutFrames OBJECT-TYPE
    SYNTAX  Counter
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
        "The total number of  packets transmitted out of the
        interface."
    ::= { pf5000EtherTxQoSStatsEntry 4 }

pf5000EtherTxQoSStatsTotalOutBytesHigh OBJECT-TYPE
    SYNTAX  Counter
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
        "The total number of octets transmitted out of the
        interface."
    ::= { pf5000EtherTxQoSStatsEntry 5 }

pf5000EtherTxQoSStatsTotalOutBytesLow OBJECT-TYPE
    SYNTAX  Counter
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
        "The total number of octets transmitted out of the
        interface."
    ::= { pf5000EtherTxQoSStatsEntry 6 }

pf5000EtherTxQoSStatsTotalDiscardFrames OBJECT-TYPE
    SYNTAX  Counter
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
        "The number of outbound packets which were chosen
        to be discarded ."
    ::= { pf5000EtherTxQoSStatsEntry 7 }

pf5000EtherTxQoSStatsQueueTable OBJECT-TYPE
    SYNTAX  SEQUENCE OF Pf5000EtherTxQoSStatsQueueEntry
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
        "A collection of priority queue statistics."
    ::= { pf5000EtherTxQoS 2 }

pf5000EtherTxQoSStatsQueueEntry OBJECT-TYPE
    SYNTAX  Pf5000EtherTxQoSStatsQueueEntry
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
        "A entry of priority queue statistics."
    INDEX { pf5000EtherTxQoSStatsQueueIndex,
            pf5000EtherTxQoSStatsQueueQueIndex }
    ::= { pf5000EtherTxQoSStatsQueueTable 1 }

    Pf5000EtherTxQoSStatsQueueEntry ::=
        SEQUENCE {
            pf5000EtherTxQoSStatsQueueIndex
                INTEGER,
            pf5000EtherTxQoSStatsQueueQueIndex
                INTEGER,
            pf5000EtherTxQoSStatsQueueQlen
                INTEGER,
            pf5000EtherTxQoSStatsQueueMaxQlen
                INTEGER,
            pf5000EtherTxQoSStatsQueueDiscardFramesClass1
                Counter64,
            pf5000EtherTxQoSStatsQueueDiscardFramesClass2
                Counter64,
            pf5000EtherTxQoSStatsQueueDiscardFramesClass3
                Counter64,
            pf5000EtherTxQoSStatsQueueDiscardFramesClass4
                Counter64
        }

pf5000EtherTxQoSStatsQueueIndex OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
        "Identified by a particular value of this
        index is same value of ifIndex.Its value ranges
        between 1 and the value of ifNumber."
    ::= { pf5000EtherTxQoSStatsQueueEntry 1 }

pf5000EtherTxQoSStatsQueueQueIndex OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
        "An index that uniquely identifies the particular
        queue.Its value ranges between 1 and the value of
        pf5000EtherTxQoSStatsMaxQnum."
    ::= { pf5000EtherTxQoSStatsQueueEntry 2 }

pf5000EtherTxQoSStatsQueueQlen OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
        "A length of outbound priority queue for a interface."
    ::= { pf5000EtherTxQoSStatsQueueEntry 3 }

pf5000EtherTxQoSStatsQueueMaxQlen OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
        "Max length of outband priority queue for a interface since
        system was up."
    ::= { pf5000EtherTxQoSStatsQueueEntry 4 }

pf5000EtherTxQoSStatsQueueDiscardFramesClass1 OBJECT-TYPE
    SYNTAX  Counter64
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
        "The number of discard frames in the discard class 
         1 of output priority queue."
    ::= { pf5000EtherTxQoSStatsQueueEntry 5 }

pf5000EtherTxQoSStatsQueueDiscardFramesClass2 OBJECT-TYPE
    SYNTAX  Counter64
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
        "The number of discard frames in the discard class 
         2 of output priority queue."
    ::= { pf5000EtherTxQoSStatsQueueEntry 6 }

pf5000EtherTxQoSStatsQueueDiscardFramesClass3 OBJECT-TYPE
    SYNTAX  Counter64
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
        "The number of discard frames in the discard class 
         3 of output priority queue."
    ::= { pf5000EtherTxQoSStatsQueueEntry 7 }

pf5000EtherTxQoSStatsQueueDiscardFramesClass4 OBJECT-TYPE
    SYNTAX  Counter64
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
        "The number of discard frames in the discard class 
         4 of output priority queue."
    ::= { pf5000EtherTxQoSStatsQueueEntry 8 }

