    SYNTAX  IpAddress
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
        "A integer  uniquely  identifying  the neighboring
         router in the Autonomous System."
    ::= { pf5000OspfVirtNbrEntry 3 }

pf5000OspfVirtNbrIpAddr OBJECT-TYPE
    SYNTAX  IpAddress
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
        "The IP address this Virtual Neighbor is using."
    ::= { pf5000OspfVirtNbrEntry 4 }

pf5000OspfVirtNbrOptions OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
        "A Bit Mask corresponding to the neighbor's options
         field.

         Bit 1, if set, indicates that the  system  will operate
         on Type of Service metrics other than TOS 0. If zero,
         the neighbor will  ignore  all metrics except the TOS
         0 metric.

         Bit 2, if set, indicates  that  the  system  is Network
         Multicast  capable; ie, that it implements OSPF Multicast
         Routing."
    ::= { pf5000OspfVirtNbrEntry 5 }

pf5000OspfVirtNbrState OBJECT-TYPE
    SYNTAX  INTEGER {
        down (1),
        attempt (2),
        init (3),
        twoWay (4),
        exchangeStart (5),
        exchange (6),
        loading (7),
        full (8)
    }
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
        "The state of the  Virtual  Neighbor  Relationship."
    ::= { pf5000OspfVirtNbrEntry 6 }

pf5000OspfVirtNbrEvents OBJECT-TYPE
    SYNTAX  Counter
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
        "The number of times this virtual link has changed
        its state, or an error has occurred."
    ::= { pf5000OspfVirtNbrEntry 7 }

pf5000OspfVirtNbrLsRetransQLen OBJECT-TYPE
    SYNTAX  Gauge
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
        "The current length of the retransmission queue."
    ::= { pf5000OspfVirtNbrEntry 8 }

pf5000OspfExtLsdbTable OBJECT-TYPE
    SYNTAX  SEQUENCE OF Pf5000OspfExtLsdbEntry
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
        "The OSPF Process's Links State Database."
    ::= { pf5000Ospf 12 }

pf5000OspfExtLsdbEntry OBJECT-TYPE
    SYNTAX  Pf5000OspfExtLsdbEntry
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
        "A single Link State Advertisement."
    INDEX { pf5000OspfExtLsdbDomainNumber,
            pf5000OspfExtLsdbType,
            pf5000OspfExtLsdbLsid,
            pf5000OspfExtLsdbRouterId }
    ::= { pf5000OspfExtLsdbTable 1 }

    Pf5000OspfExtLsdbEntry ::=
        SEQUENCE {
            pf5000OspfExtLsdbDomainNumber    INTEGER,
            pf5000OspfExtLsdbType            INTEGER,
            pf5000OspfExtLsdbLsid            IpAddress,
            pf5000OspfExtLsdbRouterId        IpAddress,
            pf5000OspfExtLsdbSequence        INTEGER,
            pf5000OspfExtLsdbAge             INTEGER,
            pf5000OspfExtLsdbChecksum        INTEGER,
            pf5000OspfExtLsdbAdvertisement   OCTET STRING
        }

pf5000OspfExtLsdbDomainNumber OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
        "OSPF domain number."
    ::= { pf5000OspfExtLsdbEntry 1 }

pf5000OspfExtLsdbType OBJECT-TYPE
    SYNTAX  INTEGER {
        asExternalLink (5)
    }
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
        "The type  of  the  link  state  advertisement."
    ::= { pf5000OspfExtLsdbEntry 2 }

pf5000OspfExtLsdbLsid OBJECT-TYPE
    SYNTAX  IpAddress
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
        "The Link State ID is an LS Type Specific field
         containing either a Router ID or an IP Address;
         it identifies the piece of the  routing  domain
         that is being described by the advertisement."
    ::= { pf5000OspfExtLsdbEntry 3 }

pf5000OspfExtLsdbRouterId OBJECT-TYPE
    SYNTAX  IpAddress
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
        "The 32 bit number that uniquely identifies the
        originating router in the Autonomous System."
    ::= { pf5000OspfExtLsdbEntry 4 }

pf5000OspfExtLsdbSequence OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
        "The sequence number field is a  signed  32-bit integer."
    ::= { pf5000OspfExtLsdbEntry 5 }

pf5000OspfExtLsdbAge OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
        "The age of the link state advertisement in seconds."
    ::= { pf5000OspfExtLsdbEntry 6 }

pf5000OspfExtLsdbChecksum OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
        "This field is the  checksum  of  the  complete
         contents  of  the  advertisement, excepting the
         age field.  The age field is excepted  so  that
         an   advertisement's  age  can  be  incremented
         without updating the  checksum."
    ::= { pf5000OspfExtLsdbEntry 7 }

pf5000OspfExtLsdbAdvertisement OBJECT-TYPE
    SYNTAX  OCTET STRING
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
        "The entire Link State Advertisement, including its header."
    ::= { pf5000OspfExtLsdbEntry 8 }

pf5000OspfAreaAggregateTable OBJECT-TYPE
    SYNTAX  SEQUENCE OF Pf5000OspfAreaAggregateEntry
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
        "A range of IP addresses  specified  by  an  IP
        address/IP  network  mask  pair."
    ::= { pf5000Ospf 14 }

pf5000OspfAreaAggregateEntry OBJECT-TYPE
    SYNTAX  Pf5000OspfAreaAggregateEntry
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
        "A range of IP addresses  specified  by  an  IP
        address/IP  network  mask  pair."
    INDEX { pf5000OspfAreaAggregateDomainNumber,
            pf5000OspfAreaAggregateAreaID,
            pf5000OspfAreaAggregateLsdbType,
            pf5000OspfAreaAggregateNet,
            pf5000OspfAreaAggregateMask }
    ::= { pf5000OspfAreaAggregateTable 1 }

    Pf5000OspfAreaAggregateEntry ::=
        SEQUENCE {
            pf5000OspfAreaAggregateDomainNumber  INTEGER,
            pf5000OspfAreaAggregateAreaID        IpAddress,
            pf5000OspfAreaAggregateLsdbType      INTEGER,
            pf5000OspfAreaAggregateNet           IpAddress,
            pf5000OspfAreaAggregateMask          IpAddress,
            pf5000OspfAreaAggregateStatus        INTEGER,
            pf5000OspfAreaAggregateEffect        INTEGER
        }

pf5000OspfAreaAggregateDomainNumber OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
        "OSPF domain number."
    ::= { pf5000OspfAreaAggregateEntry 1 }

pf5000OspfAreaAggregateAreaID OBJECT-TYPE
    SYNTAX  IpAddress
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
        "The Area the Address Aggregate is to be found within."
    ::= { pf5000OspfAreaAggregateEntry 2 }

pf5000OspfAreaAggregateLsdbType OBJECT-TYPE
    SYNTAX  INTEGER {
        summaryLink (3),
        nssaExternalLink (7)
    }
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
        "The type of the Address Aggregate. This field
         specifies  the  Lsdb type that this Address 
         Aggregate applies to."
    ::= { pf5000OspfAreaAggregateEntry 3 }

pf5000OspfAreaAggregateNet OBJECT-TYPE
    SYNTAX  IpAddress
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
        "The IP Address of the Net or Subnet indicated by the range."
    ::= { pf5000OspfAreaAggregateEntry 4 }

pf5000OspfAreaAggregateMask OBJECT-TYPE
    SYNTAX  IpAddress
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
        "The Subnet Mask that pertains to the Net or Subnet."
    ::= { pf5000OspfAreaAggregateEntry 5 }

pf5000OspfAreaAggregateStatus OBJECT-TYPE
    SYNTAX  INTEGER {
        active(1)
    }
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
        "This variable displays the status of  the  entry.
        This entry value is only valid(1)."
    ::= { pf5000OspfAreaAggregateEntry 6 }

pf5000OspfAreaAggregateEffect OBJECT-TYPE
    SYNTAX  INTEGER {
        advertiseMatching (1),
        doNotAdvertiseMatching (2)
    }
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
        "Subnets subsumed by ranges either trigger  the
         advertisement  of  the indicated aggregate (ad-
         vertiseMatching), or result in the subnet's not
         being advertised at all outside the area."
    ::= { pf5000OspfAreaAggregateEntry 7 }

pf5000OspfTrapControlTable OBJECT-TYPE
    SYNTAX  SEQUENCE OF Pf5000OspfTrapControlEntry
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
        "The table about trap information."
    ::= { pf5000OspfTrap 1 }

pf5000OspfTrapControlEntry OBJECT-TYPE
    SYNTAX  Pf5000OspfTrapControlEntry
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
        "The Entry about trap information."
    INDEX { pf5000OspfTrapDomainNumber }
    ::= { pf5000OspfTrapControlTable 1 }

    Pf5000OspfTrapControlEntry ::=
    SEQUENCE {
        pf5000OspfTrapDomainNumber
            INTEGER,
        pf5000OspfSetTrap
            OCTET STRING,
        pf5000OspfConfigErrorType
            INTEGER,
        pf5000OspfPacketType
            INTEGER,
        pf5000OspfPacketSrc
            IpAddress
    }

pf5000OspfTrapDomainNumber OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
        "pf5000OspfTrap domain number."
    ::= { pf5000OspfTrapControlEntry 1 }

pf5000OspfSetTrap OBJECT-TYPE
    SYNTAX  OCTET STRING
    ACCESS  read-write
    STATUS  mandatory
    DESCRIPTION
        "A four-octet string serving as a bit  map  for
        the trap events defined by the OSPF traps. This
        object is used to enable and  disable  specific
        OSPF   traps   where  a  1  in  the  bit  field
        represents enabled.  The right-most bit  (least
        significant) represents trap 0."
    ::= { pf5000OspfTrapControlEntry 2 }

pf5000OspfConfigErrorType OBJECT-TYPE
    SYNTAX  INTEGER {
        badVersion (1),
        areaMismatch (2),
        unknownNbmaNbr (3), -- Router is Dr eligible
        unknownVirtualNbr (4),
        authTypeMismatch(5),
        authFailure (6),
        netMaskMismatch (7),
        helloIntervalMismatch (8),
        deadIntervalMismatch (9),
        optionMismatch (10),
        duplicateRouterId (12),
        noError (13)
    }
    ACCESS   read-only
    STATUS   mandatory
    DESCRIPTION
        "Potential types  of  configuration  conflicts.
        Used  by the ospfConfigError and ospfConfigVir-
        tError traps."
    ::= { pf5000OspfTrapControlEntry 3 }

pf5000OspfPacketType OBJECT-TYPE
    SYNTAX  INTEGER {
        hello (1),
        dbDescript (2),
        lsReq (3),
        lsUpdate (4),
        lsAck (5), 
        nullPacket (6)
    }
    ACCESS   read-only
    STATUS   mandatory
    DESCRIPTION
        "OSPF packet types."
    ::= { pf5000OspfTrapControlEntry 4 }

pf5000OspfPacketSrc OBJECT-TYPE
    SYNTAX   IpAddress
    ACCESS   read-only
    STATUS   mandatory
    DESCRIPTION
        "The IP address of an inbound packet that  can-
        not be identified by a neighbor instance."
    ::= { pf5000OspfTrapControlEntry 5 }

-- pf5000Ospfv3

pf5000Ospfv3GeneralTable OBJECT-TYPE
    SYNTAX  SEQUENCE OF Pf5000Ospfv3GeneralEntry
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
        "The table which ospfv3 information is stored in."
    ::= { pf5000Ospfv3 1 }

pf5000Ospfv3GeneralEntry OBJECT-TYPE
    SYNTAX  Pf5000Ospfv3GeneralEntry
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
        "The list of the ospfv3 information."
    INDEX { pf5000Ospfv3GeneralDomainNumber }
    ::= { pf5000Ospfv3GeneralTable 1 }

    Pf5000Ospfv3GeneralEntry ::=
    SEQUENCE {
        pf5000Ospfv3GeneralDomainNumber
            INTEGER,
        pf5000Ospfv3RouterId
            IpAddress,
        pf5000Ospfv3AdminStat
            INTEGER,
        pf5000Ospfv3VersionNumber
            INTEGER,
        pf5000Ospfv3AreaBdrRtrStatus
            INTEGER,
        pf5000Ospfv3ASBdrRtrStatus
            INTEGER,
        pf5000Ospfv3AsScopeLsaCount
            Gauge,
        pf5000Ospfv3AsScopeLsaCksumSum
            INTEGER,
        pf5000Ospfv3OriginateNewLsas
            Counter,
        pf5000Ospfv3RxNewLsas
            Counter,
        pf5000Ospfv3ExtAreaLsdbLimit
            INTEGER,
        pf5000Ospfv3MulticastExtensions
            INTEGER,
        pf5000Ospfv3DemandExtensions
            INTEGER,
        pf5000Ospfv3TrafficEngineeringSupport
            INTEGER
    }

pf5000Ospfv3GeneralDomainNumber OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
        "ospfv3 domain number."
    ::= { pf5000Ospfv3GeneralEntry 1 }

pf5000Ospfv3RouterId OBJECT-TYPE
    SYNTAX  IpAddress
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
        "A  32-bit  integer  uniquely  identifying  the
        router in the Autonomous System.
        By convention, to  ensure  uniqueness, this
        should  default  to  the  value  of  one of the
        router's IPv4 interface addresses if IPv4 is
        configured."
    ::= { pf5000Ospfv3GeneralEntry 2 }

pf5000Ospfv3AdminStat OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
        "The  administrative  status  of  pf5000Ospfv3  in  the
        router.  The  value 'enabled' denotes that the
        pf5000Ospfv3 Process is active on at least  one  inter-
        face;  'disabled'  disables  it  on  all inter-
        faces."
    ::= { pf5000Ospfv3GeneralEntry 3 }

pf5000Ospfv3VersionNumber OBJECT-TYPE
    SYNTAX  INTEGER { version3 (3) }
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
        "The version number of OSPF for IPv6 is 3."
    ::= { pf5000Ospfv3GeneralEntry 4 }

pf5000Ospfv3AreaBdrRtrStatus OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
        "A flag to note whether this router is an  area
        border router."
    REFERENCE
        "OSPF Version 2, Section 3 Splitting the AS into
        Areas"
    ::= { pf5000Ospfv3GeneralEntry 5 }

pf5000Ospfv3ASBdrRtrStatus OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
        "A flag to note whether this router is  config-
        ured as an Autonomous System border router."
    REFERENCE
        "OSPF Version 2, Section 3.3  Classification  of
        routers"
    ::= { pf5000Ospfv3GeneralEntry 6 }

pf5000Ospfv3AsScopeLsaCount OBJECT-TYPE
    SYNTAX  Gauge
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
        "The number of AS-Scope (e.g. AS-External) link-state
        advertisements in the link-state database."
    ::= { pf5000Ospfv3GeneralEntry 7 }

pf5000Ospfv3AsScopeLsaCksumSum OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
        "The 32-bit unsigned sum of the LS checksums of
        the  AS-scoped link-state advertisements  con-
        tained in the link-state database. This sum
        can  be  used  to determine if there has been a
        change in a router's link state  database,  and
        to  compare  the  link-state  database  of  two
        routers."
    ::= { pf5000Ospfv3GeneralEntry 8 }

pf5000Ospfv3OriginateNewLsas OBJECT-TYPE
    SYNTAX  Counter
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
        "The number of  new  link-state  advertisements
        that  have been originated.  This number is in-
        cremented each time the router originates a new
        LSA."
    ::= { pf5000Ospfv3GeneralEntry 9 }

pf5000Ospfv3RxNewLsas OBJECT-TYPE
    SYNTAX  Counter
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
        "The number of  link-state  advertisements  re-
        ceived  determined  to  be  new instantiations.
        This number does not include  newer  instantia-
        tions  of self-originated link-state advertise-
        ments."
    ::= { pf5000Ospfv3GeneralEntry 10 }

pf5000Ospfv3ExtAreaLsdbLimit OBJECT-TYPE
    SYNTAX  INTEGER (-1..'7FFFFFFF'h)
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
        "The  maximum   number   of   non-default   AS-
        external-LSAs entries that can be stored in the
        link-state database.  If the value is -1,  then
        there is no limit.
        When the number of non-default AS-external-LSAs
        in  a  router's  link-state  database  reaches
        pf5000Ospfv3ExtAreaLsdbLimit, the router  enters  Overflow-
        State.  The   router  never  holds  more  than
        pf5000Ospfv3ExtAreaLsdbLimit  non-default  AS-external-LSAs
        in  its  database. OspfExtAreaLsdbLimit MUST be set
        identically in all routers attached to the pf5000Ospfv3
        backbone  and/or  any regular pf5000Ospfv3 area. (i.e.,
        pf5000Ospfv3 stub areas and NSSAs are excluded)."
    DEFVAL { -1 }
    ::= { pf5000Ospfv3GeneralEntry 11 }

pf5000Ospfv3MulticastExtensions OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
        "A Bit Mask indicating whether  the  router  is
        forwarding  IPv6  multicast datagrams
        based on the algorithms defined in  the  Multi-
        cast Extensions to OSPF.
        Bit 0, if set, indicates that  the  router  can
        forward  IPv6 multicast datagrams in the router's
        directly attached areas (called intra-area mul-
        ticast routing).
        Bit 1, if set, indicates that  the  router  can
        forward  IPv6  multicast  datagrams  between pf5000Ospfv3
        areas (called inter-area multicast routing).
        Bit 2, if set, indicates that  the  router  can
        forward  IPv6  multicast  datagrams between Auto-
        nomous Systems (called inter-AS multicast rout-
        ing).
        Only certain combinations of bit  settings are
        allowed,  namely: 0 (no multicast forwarding is
        enabled), 1 (intra-area multicasting  only),  3
        (intra-area  and  inter-area  multicasting),  5
        (intra-area and inter-AS  multicasting)  and  7
        (multicasting  everywhere). By default, no mul-
        ticast forwarding is enabled."
    DEFVAL { 0 }
    ::= { pf5000Ospfv3GeneralEntry 12 }

pf5000Ospfv3DemandExtensions OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
        "The router's support for demand routing."
    REFERENCE
        "OSPF Version 2, Appendix on Demand Routing"
    ::= { pf5000Ospfv3GeneralEntry 14 }

pf5000Ospfv3TrafficEngineeringSupport OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
        "The router's support for traffic engineering
        extensions."
    ::= { pf5000Ospfv3GeneralEntry 15 }

--  The pf5000Ospfv3 Area Data Structure contains information
--  regarding the various areas. The interfaces and
--  virtual links are configured as part of these areas.
--  Area 0.0.0.0, by definition, is the Backbone Area

pf5000Ospfv3AreaTable OBJECT-TYPE
    SYNTAX  SEQUENCE OF Pf5000Ospfv3AreaEntry
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
        "Information describing the configured  parame-
        ters  and cumulative statistics of the router's
        attached areas."
    REFERENCE
        "OSPF Version 2, Section 6  The Area Data Struc-
        ture"
    ::= { pf5000Ospfv3 2 }

pf5000Ospfv3AreaEntry OBJECT-TYPE
    SYNTAX  Pf5000Ospfv3AreaEntry
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
        "Information describing the configured  parame-
        ters  and  cumulative  statistics of one of the
        router's attached areas."
        INDEX { pf5000Ospfv3AreaDomainNumber,
                pf5000Ospfv3AreaId }
    ::= { pf5000Ospfv3AreaTable 1 }

    Pf5000Ospfv3AreaEntry ::=
        SEQUENCE {
            pf5000Ospfv3AreaDomainNumber
                INTEGER,
            pf5000Ospfv3AreaId
                IpAddress,
            pf5000Ospfv3ImportAsExtern
                INTEGER,
            pf5000Ospfv3SpfRuns
                Counter,
            pf5000Ospfv3AreaBdrRtrCount
                Gauge,
            pf5000Ospfv3AsBdrRtrCount
                Gauge,
            pf5000Ospfv3AreaScopeLsaCount
                Gauge,
            pf5000Ospfv3AreaScopeLsaCksumSum
                INTEGER,
            pf5000Ospfv3AreaSummary
                INTEGER,
            pf5000Ospfv3AreaStatus
                INTEGER,
            pf5000Ospfv3StubMetric
                INTEGER
        }

pf5000Ospfv3AreaDomainNumber OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
        "ospfv3 domain number."
    ::= { pf5000Ospfv3AreaEntry 1 }

pf5000Ospfv3AreaId OBJECT-TYPE
    SYNTAX  IpAddress
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
        "A 32-bit integer uniquely identifying an area.
        Area ID 0.0.0.0 is used for the pf5000Ospfv3 backbone."
    REFERENCE
        "OSPF Version 2, Appendix C.2 Area parameters"
    ::= { pf5000Ospfv3AreaEntry 2 }

pf5000Ospfv3ImportAsExtern OBJECT-TYPE
    SYNTAX  INTEGER {
        importExternal (1),   -- normal area
        importNoExternal (2), -- stub area
        importNssa (3)        -- not-so-stubby-area
    }
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
        "Indicates whether an area is a Stub area, NSSA, or
        standard area. AS-scope LSAs are not imported into Stub
        Areas or NSSAs. NSSAs import AS-External data as Type-7
        LSAs which have Area-scope"
    REFERENCE
        "OSPF Version 2, Appendix C.2 Area parameters"
        DEFVAL { importExternal }
    ::= { pf5000Ospfv3AreaEntry 3 }

pf5000Ospfv3SpfRuns OBJECT-TYPE
    SYNTAX  Counter
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
        "The number of times that the intra-area  route
        table  has  been  calculated  using this area's
        link-state database.  This  is  typically  done
        using Dijkstra's algorithm."
    ::= { pf5000Ospfv3AreaEntry 4 }

pf5000Ospfv3AreaBdrRtrCount OBJECT-TYPE
    SYNTAX  Gauge
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
        "The total number of area border routers reach-
        able within this area. This is initially zero,
        and is calculated in each SPF Pass."
    ::= { pf5000Ospfv3AreaEntry 5 }

pf5000Ospfv3AsBdrRtrCount OBJECT-TYPE
    SYNTAX  Gauge
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
        "The total number of Autonomous  System  border
        routers  reachable  within  this area.  This is
        initially zero, and is calculated in  each  SPF
        Pass."
    ::= { pf5000Ospfv3AreaEntry 6 }

pf5000Ospfv3AreaScopeLsaCount OBJECT-TYPE
    SYNTAX  Gauge
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
    "The total number of Area-Scope link-state
    advertisements in  this  area's link-state
    database."
    ::= { pf5000Ospfv3AreaEntry 7 }

pf5000Ospfv3AreaScopeLsaCksumSum OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
        "The 32-bit unsigned sum of the Area-Scope link-state
        advertisements'  LS  checksums  contained in this
        area's link-state database.  The sum can be used
        to determine if  there  has been  a  change  in a
        router's link state database, and to compare the
        link-state database of two routers."
        DEFVAL  { 0 }
    ::= { pf5000Ospfv3AreaEntry 8 }

pf5000Ospfv3AreaSummary OBJECT-TYPE
    SYNTAX  INTEGER {
        noAreaSummary (1),
        sendAreaSummary (2)
    }
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
        "The variable pf5000Ospfv3AreaSummary controls the  im-
        port  of  summary LSAs into stub areas.  It has
        no effect on other areas.
        If it is noAreaSummary, the router will neither
        originate  nor  propagate summary LSAs into the
        stub area.  It will rely entirely  on  its  de-
        fault route.
        If it is sendAreaSummary, the router will  both
        summarize and propagate summary LSAs."
        DEFVAL  { noAreaSummary }
    ::= { pf5000Ospfv3AreaEntry 9 }

pf5000Ospfv3AreaStatus OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
        "This variable displays the status of  the  en-
        try.  Setting it to 'invalid' has the effect of
        rendering it inoperative.  The internal  effect
        (row removal) is implementation dependent."
    ::= { pf5000Ospfv3AreaEntry 10 }

pf5000Ospfv3StubMetric OBJECT-TYPE
    SYNTAX  INTEGER
--    ACCESS  read-create
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
        "The metric value advertised for the default route
        into Stub and NSSA areas."
    ::= { pf5000Ospfv3AreaEntry 11 }

--    pf5000Ospfv3 AS-Scope Link State Database
--        The Link State Database contains the AS-Scope Link State
--        Advertisements from throughout the areas that the
--        device is attached to.

pf5000Ospfv3AsLsdbTable OBJECT-TYPE
    SYNTAX  SEQUENCE OF Pf5000Ospfv3AsLsdbEntry
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
        "The pf5000Ospfv3 Process's AS-Scope Link State Database."
