--  OSPF Host Table
--    The Host/Metric Table indicates what hosts are directly
--    attached to the Router, and what metrics and types of
--    service should be advertised for them.
--  pf5000Ospfv3 Interface Table

pf5000Ospfv3IfTable OBJECT-TYPE
    SYNTAX  SEQUENCE OF Pf5000Ospfv3IfEntry
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
        "The pf5000Ospfv3 Interface Table describes the  inter-
        faces from the viewpoint of pf5000Ospfv3."
    REFERENCE
        "OSPF Version 2, Appendix C.3  Router  interface
        parameters"
    ::= { pf5000Ospfv3 7 }

pf5000Ospfv3IfEntry OBJECT-TYPE
    SYNTAX  Pf5000Ospfv3IfEntry
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
        "The pf5000Ospfv3 Interface Entry describes one  inter-
        face from the viewpoint of pf5000Ospfv3."
    INDEX { pf5000Ospfv3IfDomainNumber,
            pf5000Ospfv3IfIndex }
    ::= { pf5000Ospfv3IfTable 1 }

    Pf5000Ospfv3IfEntry ::=
        SEQUENCE {
            pf5000Ospfv3IfDomainNumber
                INTEGER,
            pf5000Ospfv3IfIndex
                INTEGER,
            pf5000Ospfv3IfAreaId
                IpAddress,
            pf5000Ospfv3IfType
                INTEGER,
            pf5000Ospfv3IfAdminStat
                INTEGER,
            pf5000Ospfv3IfRtrPriority
                INTEGER,
            pf5000Ospfv3IfTransitDelay
                INTEGER,
            pf5000Ospfv3IfRetransInterval
                INTEGER,
            pf5000Ospfv3IfHelloInterval
                INTEGER,
            pf5000Ospfv3IfRtrDeadInterval
                INTEGER,
            pf5000Ospfv3IfPollInterval
                INTEGER,
            pf5000Ospfv3IfState
                INTEGER,
            pf5000Ospfv3IfDesignatedRouter
                IpAddress,
            pf5000Ospfv3IfBackupDesignatedRouter
                IpAddress,
            pf5000Ospfv3IfEvents
                Counter,
            pf5000Ospfv3IfStatus
                INTEGER,
            pf5000Ospfv3IfMulticastForwarding
                INTEGER,
            pf5000Ospfv3IfDemand
                INTEGER,
            pf5000Ospfv3IfMetricValue
                INTEGER,
            pf5000Ospfv3IfLinkScopeLsaCount
                INTEGER,
            pf5000Ospfv3IfLinkLsaCksumSum
                INTEGER,
            pf5000Ospfv3IfInstId
                INTEGER
        }

pf5000Ospfv3IfDomainNumber OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
        "ospfv3 domain number."
    ::= { pf5000Ospfv3IfEntry 1 }

pf5000Ospfv3IfIndex OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
        "The interface index of this pf5000Ospfv3 interface.
        It corresponds to the interface index of the
        Ipv6 interface on which pf5000Ospfv3 is configured."
    ::= { pf5000Ospfv3IfEntry 2 }

pf5000Ospfv3IfAreaId OBJECT-TYPE
    SYNTAX  IpAddress
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
        "A 32-bit integer uniquely identifying the area
        to  which  the  interface  connects.   Area  ID
        0.0.0.0 is used for the pf5000Ospfv3 backbone."
    DEFVAL   { '00000000'H }  -- 0.0.0.0
    ::= { pf5000Ospfv3IfEntry 3 }

pf5000Ospfv3IfType OBJECT-TYPE
    SYNTAX  INTEGER {
        broadcast (1),
        nbma (2),
        pointToPoint (3),
        pointToMultipoint (5)
    }
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
        "The pf5000Ospfv3 interface type."
    ::= { pf5000Ospfv3IfEntry 4 }

pf5000Ospfv3IfAdminStat OBJECT-TYPE
    SYNTAX  INTEGER {
        enabled (1),
        disabled (2)
    }
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
        "The pf5000Ospfv3 interface's  administrative  status.
        The  value formed on the interface, and the in-
        terface will be advertised as an internal route
        to  some  area.  The  value 'disabled' denotes
        that the interface is external to pf5000Ospfv3."
    DEFVAL { enabled }
    ::= { pf5000Ospfv3IfEntry 5 }

pf5000Ospfv3IfRtrPriority OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
        "The  priority  of  this  interface.   Used  in
        multi-access  networks,  this  field is used in
        the designated router election algorithm.  The
        value 0 signifies that the router is not eligi-
        ble to become the  designated  router  on  this
        particular  network.   In the event of a tie in
        this value, routers will use their Router ID as
        a tie breaker."
    DEFVAL { 1 }
    ::= { pf5000Ospfv3IfEntry 6 }

pf5000Ospfv3IfTransitDelay OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
        "The estimated number of seconds  it  takes  to
        transmit  a  link state update packet over this
        interface."
    DEFVAL { 1 }
    ::= { pf5000Ospfv3IfEntry 7 }

pf5000Ospfv3IfRetransInterval OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
        "The number of seconds between  link-state  ad-
        vertisement  retransmissions,  for  adjacencies
        belonging to this  interface.  This  value  is
        also used when retransmitting database descrip-
        tion and link-state request packets."
    DEFVAL { 5 }
    ::= { pf5000Ospfv3IfEntry 8 }

pf5000Ospfv3IfHelloInterval OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
        "The length of time, in  seconds,  between  the
        Hello  packets that the router sends on the in-
        terface.  This value must be the same  for  all
        routers attached to a common network."
    DEFVAL { 10 }
    ::= { pf5000Ospfv3IfEntry 9 }

pf5000Ospfv3IfRtrDeadInterval OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
        "The number of seconds that  a  router's  Hello
        packets  have  not been seen before it's neigh-
        bors declare the router down.  This  should  be
        some  multiple  of  the  Hello  interval.  This
        value must be the same for all routers attached
        to a common network."
    DEFVAL { 40 }
    ::= { pf5000Ospfv3IfEntry 10 }

pf5000Ospfv3IfPollInterval OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
        "The larger time interval, in seconds,  between
        the  Hello  packets  sent  to  an inactive non-
        broadcast multi- access neighbor."
    DEFVAL { 120 }
    ::= { pf5000Ospfv3IfEntry 11 }

pf5000Ospfv3IfState OBJECT-TYPE
    SYNTAX  INTEGER {
        down (1),
        loopback (2),
        waiting (3),
        pointToPoint (4),
        designatedRouter (5),
        backupDesignatedRouter (6),
        otherDesignatedRouter (7)
    }
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
        "The pf5000Ospfv3 Interface State."
    DEFVAL { down }
    ::= { pf5000Ospfv3IfEntry 12 }

pf5000Ospfv3IfDesignatedRouter OBJECT-TYPE
    SYNTAX  IpAddress
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
        "The Router ID of the Designated Router."
    DEFVAL  { '00000000'H }  -- 0.0.0.0
    ::= { pf5000Ospfv3IfEntry 13 }

pf5000Ospfv3IfBackupDesignatedRouter OBJECT-TYPE
    SYNTAX  IpAddress
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
        "The  Router ID of  the  Backup   Designated
        Router."
    DEFVAL  { '00000000'H }  -- 0.0.0.0
    ::= { pf5000Ospfv3IfEntry 15 }

pf5000Ospfv3IfEvents OBJECT-TYPE
    SYNTAX  Counter
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
        "The number of times this  OSPF  interface  has
        changed its state, or an error has occurred."
    ::= { pf5000Ospfv3IfEntry 16 }

pf5000Ospfv3IfStatus OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
        "This variable displays the status of  the  en-
        try.  Setting it to 'invalid' has the effect of
        rendering it inoperative.  The internal  effect
        (row removal) is implementation dependent."
    ::= { pf5000Ospfv3IfEntry 18 }

pf5000Ospfv3IfMulticastForwarding OBJECT-TYPE
    SYNTAX  INTEGER {
        blocked (1),   -- no multicast forwarding
        multicast (2), -- using multicast address
        unicast (3)    -- to each pf5000Ospfv3 neighbor
    }
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
        "The way multicasts should  forwarded  on  this
        interface;  not  forwarded,  forwarded  as data
        link multicasts, or forwarded as data link uni-
        casts.  Data link multicasting is not meaning-
        ful on point to point and NBMA interfaces, and
        setting pf5000Ospfv3MulticastForwarding to 0 effective-
        ly disables all multicast forwarding."
    DEFVAL { blocked }
    ::= { pf5000Ospfv3IfEntry 19 }

pf5000Ospfv3IfDemand OBJECT-TYPE
    SYNTAX  INTEGER {
        true (1),
        false (2)
    }
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
        "Indicates whether Demand pf5000Ospfv3 procedures (hel-
        lo supression to FULL neighbors and setting the
        DoNotAge flag on proogated LSAs) should be per-
        formed on this interface."
    DEFVAL { false }
    ::= { pf5000Ospfv3IfEntry 20 }

pf5000Ospfv3IfMetricValue OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
        "The metric assigned to this interface.
        The default value of the Metric is
        10^8 / ifSpeed."
    ::= { pf5000Ospfv3IfEntry 21 }

pf5000Ospfv3IfLinkScopeLsaCount OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
        "The total number of Link-Scope link-state
        advertisements in  this  link's link-state database."
    ::= { pf5000Ospfv3IfEntry 22 }

pf5000Ospfv3IfLinkLsaCksumSum OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
        "The 32-bit unsigned sum of the Link-Scope link-state
        advertisements'  LS  checksums  contained in this
        link's link-state database.  The sum can be used
        to determine if  there  has been  a  change  in a
        router's link state database, and to compare the
        link-state database of two routers."
    DEFVAL  { 0 }
    ::= { pf5000Ospfv3IfEntry 23 }

pf5000Ospfv3IfInstId OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
        "Enables multiple instances of pf5000Ospfv3 to be run over
        a single link. Each protocol instance would be assigned
        a separate ID. This ID has local link significance
        only."
    DEFVAL  { 0 }
    ::= { pf5000Ospfv3IfEntry 24 }

--  pf5000Ospfv3 Virtual Interface Table
--    The Virtual Interface Table describes the virtual
--    links that the pf5000Ospfv3 Process is configured to
--    carry on.

pf5000Ospfv3VirtIfTable OBJECT-TYPE
    SYNTAX  SEQUENCE OF Pf5000Ospfv3VirtIfEntry
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
        "Information about this router's virtual inter-
        faces."
    REFERENCE
        "OSPF Version  2,  Appendix  C.4   Virtual  link
        parameters"
    ::= { pf5000Ospfv3 8 }

pf5000Ospfv3VirtIfEntry OBJECT-TYPE
    SYNTAX  Pf5000Ospfv3VirtIfEntry
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
        "Information about a single Virtual Interface."
    INDEX { pf5000Ospfv3VirtIfDomainNumber, pf5000Ospfv3VirtIfAreaId, pf5000Ospfv3VirtIfNeighbor }
    ::= { pf5000Ospfv3VirtIfTable 1 }

    Pf5000Ospfv3VirtIfEntry ::=
        SEQUENCE {
            pf5000Ospfv3VirtIfDomainNumber
                INTEGER,
            pf5000Ospfv3VirtIfAreaId
                IpAddress,
            pf5000Ospfv3VirtIfNeighbor
                IpAddress,
            pf5000Ospfv3VirtIfIndex
                INTEGER,
            pf5000Ospfv3VirtIfTransitDelay
                INTEGER,
            pf5000Ospfv3VirtIfRetransInterval
                INTEGER,
            pf5000Ospfv3VirtIfHelloInterval
                INTEGER,
            pf5000Ospfv3VirtIfRtrDeadInterval
                INTEGER,
            pf5000Ospfv3VirtIfState
                INTEGER,
            pf5000Ospfv3VirtIfEvents
                Counter,
            pf5000Ospfv3VirtIfStatus
                INTEGER,
            pf5000Ospfv3VirtIfLinkScopeLsaCount
                INTEGER,
            pf5000Ospfv3VirtIfLinkLsaCksumSum
                INTEGER
        }

pf5000Ospfv3VirtIfDomainNumber OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
        "ospfv3 domain number."
    ::= { pf5000Ospfv3VirtIfEntry 1 }

pf5000Ospfv3VirtIfAreaId OBJECT-TYPE
    SYNTAX  IpAddress
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
        "The  Transit  Area  that  the   Virtual   Link
        traverses.  By definition, this is not 0.0.0.0"
    ::= { pf5000Ospfv3VirtIfEntry 2 }

pf5000Ospfv3VirtIfNeighbor OBJECT-TYPE
    SYNTAX  IpAddress
    ACCESS  read-only
    STATUS mandatory
    DESCRIPTION
        "The Router ID of the Virtual Neighbor."
    ::= { pf5000Ospfv3VirtIfEntry 3 }

pf5000Ospfv3VirtIfIndex OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
        "The interface index of this pf5000Ospfv3 virtual interface.
        It must be unique among all the interface indices
        on the router."
        ::= { pf5000Ospfv3VirtIfEntry 4 }

pf5000Ospfv3VirtIfTransitDelay OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
        "The estimated number of seconds  it  takes  to
        transmit  a link- state update packet over this
        interface."
    DEFVAL { 1 }
    ::= { pf5000Ospfv3VirtIfEntry 5 }

pf5000Ospfv3VirtIfRetransInterval OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
        "The number of seconds between  link-state  ad-
        vertisement  retransmissions,  for  adjacencies
        belonging to this  interface.  This  value  is
        also used when retransmitting database descrip-
        tion  and  link-state  request  packets.   This
        value  should  be well over the expected round-
        trip time."
    DEFVAL { 5 }
    ::= { pf5000Ospfv3VirtIfEntry 6 }

pf5000Ospfv3VirtIfHelloInterval OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
        "The length of time, in  seconds,  between  the
        Hello  packets that the router sends on the in-
        terface.  This value must be the same  for  the
        virtual neighbor."
    DEFVAL { 10 }
    ::= { pf5000Ospfv3VirtIfEntry 7 }

pf5000Ospfv3VirtIfRtrDeadInterval OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
        "The number of seconds that  a  router's  Hello
        packets  have  not been seen before it's neigh-
        bors declare the router down.  This  should  be
        some  multiple  of  the  Hello  interval.  This
        value must be the same for the  virtual  neigh-
        bor."
    DEFVAL { 60 }
    ::= { pf5000Ospfv3VirtIfEntry 8 }

pf5000Ospfv3VirtIfState OBJECT-TYPE
    SYNTAX  INTEGER {
        down (1),    -- these use the same encoding
        pointToPoint (4) -- as the pf5000Ospfv3IfTable
    }
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
        "OSPF virtual interface states."
    DEFVAL  { down }
    ::= { pf5000Ospfv3VirtIfEntry 9 }

pf5000Ospfv3VirtIfEvents OBJECT-TYPE
    SYNTAX  Counter
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
        "The number of state changes or error events on
        this Virtual Link"
    ::= { pf5000Ospfv3VirtIfEntry 10 }

pf5000Ospfv3VirtIfStatus OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
        "This variable displays the status of  the  en-
        try.  Setting it to 'invalid' has the effect of
        rendering it inoperative.  The internal  effect
        (row removal) is implementation dependent."
        ::= { pf5000Ospfv3VirtIfEntry 11 }

pf5000Ospfv3VirtIfLinkScopeLsaCount OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
        "The total number of Link-Scope link-state
        advertisements in  this  virtual link's link-state
        database."
    ::= { pf5000Ospfv3VirtIfEntry 12 }

pf5000Ospfv3VirtIfLinkLsaCksumSum OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
        "The 32-bit unsigned sum of the Link-Scope link-state
        advertisements'  LS  checksums  contained in this
        virtual link's link-state database.  The sum can be used
        to determine if  there  has been  a  change  in a
        router's link state database, and to compare the
        link-state database of two routers."
    DEFVAL  { 0 }
    ::= { pf5000Ospfv3VirtIfEntry 13 }

--  pf5000Ospfv3 Neighbor Table
--    The pf5000Ospfv3 Neighbor Table describes all neighbors in
--    the locality of the subject router.

pf5000Ospfv3NbrTable OBJECT-TYPE
    SYNTAX  SEQUENCE OF Pf5000Ospfv3NbrEntry
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
        "A table of non-virtual neighbor information."
    REFERENCE
        "OSPF Version 2, Section 10  The  Neighbor  Data
        Structure"
    ::= { pf5000Ospfv3 9 }

pf5000Ospfv3NbrEntry OBJECT-TYPE
    SYNTAX  Pf5000Ospfv3NbrEntry
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
        "The information regarding a single neighbor."
    REFERENCE
        "OSPF Version 2, Section 10  The  Neighbor  Data
        Structure"
    INDEX { pf5000Ospfv3NbrDomainNumber, pf5000Ospfv3NbrIfIndex, pf5000Ospfv3NbrIpv6Addr }
    ::= { pf5000Ospfv3NbrTable 1 }

    Pf5000Ospfv3NbrEntry ::=
        SEQUENCE {
            pf5000Ospfv3NbrDomainNumber
                INTEGER,
            pf5000Ospfv3NbrIfIndex
                INTEGER,
            pf5000Ospfv3NbrIpv6Addr
                Ipv6Address,
            pf5000Ospfv3NbrRtrId
                IpAddress,
            pf5000Ospfv3NbrOptions
                INTEGER,
            pf5000Ospfv3NbrPriority
                INTEGER,
            pf5000Ospfv3NbrState
                INTEGER,
            pf5000Ospfv3NbrEvents
                Counter,
            pf5000Ospfv3NbrLsRetransQLen
                Gauge,
            pf5000Ospfv3NbrHelloSuppressed
                INTEGER,
            pf5000Ospfv3NbrIfId
                INTEGER
        }

pf5000Ospfv3NbrDomainNumber OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
        "ospfv3 domain number."
    ::= { pf5000Ospfv3NbrEntry 1 }

pf5000Ospfv3NbrIfIndex OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
        "The local link ID of the link over which the
        neighbor can be reached."
    ::= { pf5000Ospfv3NbrEntry 2 }

pf5000Ospfv3NbrIpv6Addr OBJECT-TYPE
    SYNTAX  Ipv6Address
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
        "The IPv6 address of the neighbor associated with
        the local link."
    ::= { pf5000Ospfv3NbrEntry 3 }

pf5000Ospfv3NbrRtrId OBJECT-TYPE
    SYNTAX  IpAddress
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
        "A 32-bit integer (represented as a type  IpAd-
        dress)  uniquely  identifying  the  neighboring
        router in the Autonomous System."
    ::= { pf5000Ospfv3NbrEntry 4 }

pf5000Ospfv3NbrOptions OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
        "A Bit Mask corresponding to the neighbor's op-
        tions field."
    DEFVAL { 0 }
    ::= { pf5000Ospfv3NbrEntry 5 }

pf5000Ospfv3NbrPriority OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
        "The priority of this neighbor in the designat-
        ed router election algorithm.  The value 0 sig-
        nifies that the neighbor is not eligible to be-
        come  the  designated router on this particular
        network."
    DEFVAL { 1 }
    ::= { pf5000Ospfv3NbrEntry 6 }

pf5000Ospfv3NbrState OBJECT-TYPE
    SYNTAX  INTEGER {
        down (1),
        attempt (2),
        init (3),
        twoWay (4),
        exchangeStart (5),
        exchange (6),
        loading (7),
        full (8)
    }
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
        "The State of the relationship with this Neigh-
        bor."
    REFERENCE
        "OSPF Version 2, Section 10.1 Neighbor States"
    DEFVAL  { down }
    ::= { pf5000Ospfv3NbrEntry 7 }

pf5000Ospfv3NbrEvents OBJECT-TYPE
    SYNTAX  Counter
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
        "The number of times this neighbor relationship
        has changed state, or an error has occurred."
    ::= { pf5000Ospfv3NbrEntry 8 }

pf5000Ospfv3NbrLsRetransQLen OBJECT-TYPE
    SYNTAX  Gauge
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
        "The  current  length  of  the   retransmission
        queue."
    ::= { pf5000Ospfv3NbrEntry 9 }

pf5000Ospfv3NbrHelloSuppressed OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
        "Indicates whether Hellos are being  suppressed
        to the neighbor"
    ::= { pf5000Ospfv3NbrEntry 12 }

pf5000Ospfv3NbrIfId OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
        "The interface ID that the neighbor advertises
        in its Hello Packets on this link, that is, the
        neighbor's local interface index."
    ::= { pf5000Ospfv3NbrEntry 13 }

--  pf5000Ospfv3 Virtual Neighbor Table
--    This table describes all virtual neighbors.
--    Since Virtual Links are configured in the
--    virtual interface table, this table is read-only.

pf5000Ospfv3VirtNbrTable OBJECT-TYPE
    SYNTAX  SEQUENCE OF Pf5000Ospfv3VirtNbrEntry
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
        "A table of virtual neighbor information."
    REFERENCE
        "OSPF Version 2, Section 15  Virtual Links"
    ::= { pf5000Ospfv3 10 }

pf5000Ospfv3VirtNbrEntry OBJECT-TYPE
    SYNTAX  Pf5000Ospfv3VirtNbrEntry
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
        "Virtual neighbor information."
    INDEX { pf5000Ospfv3VirtNbrDomainNumber, pf5000Ospfv3VirtNbrArea, pf5000Ospfv3VirtNbrRtrId }
    ::= { pf5000Ospfv3VirtNbrTable 1 }

    Pf5000Ospfv3VirtNbrEntry ::=
        SEQUENCE {
            pf5000Ospfv3VirtNbrDomainNumber
                INTEGER,
            pf5000Ospfv3VirtNbrArea
                IpAddress,
            pf5000Ospfv3VirtNbrRtrId
                IpAddress,
            pf5000Ospfv3VirtNbrIfIndex
                INTEGER,
            pf5000Ospfv3VirtNbrIpv6Addr
                Ipv6Address,
            pf5000Ospfv3VirtNbrOptions
                INTEGER,
            pf5000Ospfv3VirtNbrState
                INTEGER,
            pf5000Ospfv3VirtNbrEvents
                Counter,
            pf5000Ospfv3VirtNbrLsRetransQLen
                Gauge,
            pf5000Ospfv3VirtNbrHelloSuppressed
                INTEGER,
            pf5000Ospfv3VirtNbrIfId
                INTEGER
        }

pf5000Ospfv3VirtNbrDomainNumber OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
        "ospfv3 domain number."
    ::= { pf5000Ospfv3VirtNbrEntry 1 }

pf5000Ospfv3VirtNbrArea OBJECT-TYPE
    SYNTAX  IpAddress
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
        "The Transit Area Identifier."
    ::= { pf5000Ospfv3VirtNbrEntry 2 }

pf5000Ospfv3VirtNbrRtrId OBJECT-TYPE
    SYNTAX  IpAddress
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
        "A  32-bit  integer  uniquely  identifying  the
        neighboring router in the Autonomous System."
    ::= { pf5000Ospfv3VirtNbrEntry 3 }

pf5000Ospfv3VirtNbrIfIndex OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
        "The local link ID for the virtual link over
        which the neighbor can be reached."
    ::= { pf5000Ospfv3VirtNbrEntry 4 }

pf5000Ospfv3VirtNbrIpv6Addr OBJECT-TYPE
    SYNTAX  Ipv6Address
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
        "The IPv6 address used to reach this Virtual Neighbor.
        It must be a Site-Local or Global scope address."
    ::= { pf5000Ospfv3VirtNbrEntry 5 }

pf5000Ospfv3VirtNbrOptions OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
        "A Bit Mask corresponding to the neighbor's op-
        tions field."
    ::= { pf5000Ospfv3VirtNbrEntry 6 }

pf5000Ospfv3VirtNbrState OBJECT-TYPE
    SYNTAX  INTEGER {
        down (1),
        attempt (2),
        init (3),
        twoWay (4),
        exchangeStart (5),
        exchange (6),
        loading (7),
        full (8)
    }
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
        "The state of the  Virtual  Neighbor  Relation-
        ship."
    ::= { pf5000Ospfv3VirtNbrEntry 7 }

pf5000Ospfv3VirtNbrEvents OBJECT-TYPE
    SYNTAX  Counter
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
        "The number of  times  this  virtual  link  has
        changed its state, or an error has occurred."
    ::= { pf5000Ospfv3VirtNbrEntry 8 }

pf5000Ospfv3VirtNbrLsRetransQLen OBJECT-TYPE
    SYNTAX  Gauge
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
        "The  current  length  of  the   retransmission
        queue."
    ::= { pf5000Ospfv3VirtNbrEntry 9 }

pf5000Ospfv3VirtNbrHelloSuppressed OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
        "Indicates whether Hellos are being  suppressed
        to the neighbor"
    ::= { pf5000Ospfv3VirtNbrEntry 10 }

pf5000Ospfv3VirtNbrIfId OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
        "The interface ID that the neighbor advertises
        in its Hello Packets on this virtual link, that is,
        the neighbor's local interface index."
    ::= { pf5000Ospfv3VirtNbrEntry 11 }

 --
 --    The pf5000Ospfv3 Area Aggregate Table
 --

pf5000Ospfv3AreaAggregateTable OBJECT-TYPE
    SYNTAX  SEQUENCE OF Pf5000Ospfv3AreaAggregateEntry
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
        "A range of IPv6 prefixes specified by a
        prefix/prefix length  pair. Note that if
        ranges are configured such that one range sub-
        sumes  another range the most specific match is
        the preferred one."
    ::= { pf5000Ospfv3 11 }

pf5000Ospfv3AreaAggregateEntry OBJECT-TYPE
    SYNTAX  Pf5000Ospfv3AreaAggregateEntry
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
        "A range of IPv6 prefixes specified by a
        prefix/prefix length  pair. Note that if
        ranges are configured such that one range sub-
        sumes  another range the most specific match is
        the preferred one."
    REFERENCE
        "OSPF Version 2, Appendix C.2  Area parameters"
    INDEX { pf5000Ospfv3AreaAggregateDomainNumber,
            pf5000Ospfv3AreaAggregateAreaID,
            pf5000Ospfv3AreaAggregateAreaLsdbType,
            pf5000Ospfv3AreaAggregateIndex }
    ::= { pf5000Ospfv3AreaAggregateTable 1 }

    Pf5000Ospfv3AreaAggregateEntry ::=
        SEQUENCE {
            pf5000Ospfv3AreaAggregateDomainNumber
                INTEGER,
            pf5000Ospfv3AreaAggregateAreaID
                IpAddress,
            pf5000Ospfv3AreaAggregateAreaLsdbType
                INTEGER,
            pf5000Ospfv3AreaAggregateIndex
                INTEGER,
            pf5000Ospfv3AreaAggregatePrefix
                Ipv6Address,
            pf5000Ospfv3AreaAggregatePrefixLen
                INTEGER,
            pf5000Ospfv3AreaAggregateStatus
                INTEGER,
            pf5000Ospfv3AreaAggregateEffect
                INTEGER
        }

pf5000Ospfv3AreaAggregateDomainNumber OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
        "ospfv3 domain number."
    ::= { pf5000Ospfv3AreaAggregateEntry 1 }

pf5000Ospfv3AreaAggregateAreaID OBJECT-TYPE
    SYNTAX  IpAddress
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
        "The Area the Address Aggregate is to be  found
        within."
    REFERENCE
        "OSPF Version 2, Appendix C.2 Area parameters"
    ::= { pf5000Ospfv3AreaAggregateEntry 2 }

pf5000Ospfv3AreaAggregateAreaLsdbType OBJECT-TYPE
    SYNTAX  INTEGER {
        interAreaPrefixLsa (8195), -- 0x2003
        nssaExternalLsa    (8199)  -- 0x2007
    }
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
        "The type of the Address Aggregate.  This field
        specifies  the  Area Lsdb type that this Address Ag-
        gregate applies to."
    REFERENCE
        "OSPF Version 2, Appendix A.4.1 The  Link  State
        Advertisement header"
    ::= { pf5000Ospfv3AreaAggregateEntry 3 }

pf5000Ospfv3AreaAggregateIndex OBJECT-TYPE
    SYNTAX  INTEGER (1..2147483647)
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
        "A unique identifier for this aggregate table entry."
    ::= { pf5000Ospfv3AreaAggregateEntry 4 }

pf5000Ospfv3AreaAggregatePrefix OBJECT-TYPE
    SYNTAX  Ipv6Address
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
        "The IPv6 Prefix."
    REFERENCE
        "OSPF Version 2, Appendix C.2 Area parameters"
    ::= { pf5000Ospfv3AreaAggregateEntry 5 }

pf5000Ospfv3AreaAggregatePrefixLen OBJECT-TYPE
    SYNTAX  INTEGER ( 3..128 )
--    UNITS  "bits"
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
        "The length of the prefix (in bits). A prefix can
        not be shorter than 3 bits."
    REFERENCE
        "OSPF Version 2, Appendix C.2 Area parameters"
    ::= { pf5000Ospfv3AreaAggregateEntry 6 }

pf5000Ospfv3AreaAggregateStatus OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
        "This variable displays the status of  the  en-
        try.  Setting it to 'invalid' has the effect of
        rendering it inoperative.  The internal  effect
        (row removal) is implementation dependent."
    ::= { pf5000Ospfv3AreaAggregateEntry 7 }

pf5000Ospfv3AreaAggregateEffect OBJECT-TYPE
    SYNTAX  INTEGER {
        advertiseMatching (1),
        doNotAdvertiseMatching (2)
    }
    ACCESS  read-only
    STATUS  mandatory
    DESCRIPTION
        "Prefixes subsumed by ranges either trigger  the
        advertisement  of  the indicated aggregate (ad-
        vertiseMatching), or result in the prefix not
        being advertised at all outside the area."
    DEFVAL  { advertiseMatching }
    ::= { pf5000Ospfv3AreaAggregateEntry 8 }

--  pf5000Login

pf5000LoginName OBJECT-TYPE
    SYNTAX  DisplayString
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
        "Log in user name."
    ::= { pf5000Login 1 }

pf5000LoginTime OBJECT-TYPE
    SYNTAX  DisplayString
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
        "Date, time and time zone of user log in.
                 Format : YYYY/MM/DD hh:mm:ss XXXXXX(time zone)
         Time zone format :
           JST    : JST
                   GMT+09 : GMT + 09 hours
                   GMT+10 : GMT + 10 hours"
    ::= { pf5000Login 2 }

pf5000LogoutTime OBJECT-TYPE
    SYNTAX  DisplayString
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
        "Date, time and time zone of user log out.
        Format : YYYY/MM/DD hh:mm:ss XXXXXX(time zone)
        Time zone format :
          JST    : JST
          GMT+09 : GMT + 09 hours
          GMT+10 : GMT + 10 hours"
    ::= { pf5000Login 3 }

pf5000LoginFailureTime OBJECT-TYPE
    SYNTAX  DisplayString
    ACCESS  not-accessible
    STATUS  mandatory
    DESCRIPTION
        "Date, time and time zone of user log in failure.
        Format : YYYY/MM/DD hh:mm:ss XXXXXX(time zone)
        Time zone format :
          JST    : JST
          GMT+09 : GMT + 09 hours
          GMT+10 : GMT + 10 hours"
    ::= { pf5000Login 4 }

pf5000LoginLocation OBJECT-TYPE
