            pf5000OspfNbrDomainNumber       INTEGER,
            pf5000OspfNbrIpAddr             IpAddress,
            pf5000OspfNbrAddressLessIndex   INTEGER,
            pf5000OspfNbrRtrId              IpAddress,
            pf5000OspfNbrOptions            INTEGER,
            pf5000OspfNbrPriority           INTEGER,
            pf5000OspfNbrState              INTEGER,
            pf5000OspfNbrEvents             Counter,
            pf5000OspfNbrLsRetransQLen      Gauge,
            pf5000OspfNbmaNbrStatus         INTEGER,
            pf5000OspfNbmaNbrPermanence     INTEGER
        }

